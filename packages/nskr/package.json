{
  "name": "nskr",
  "version": "0.0.1",
  "description": "A simple CLI wrapper for adding components from nskr.me component registry using shadcn.",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "bin": "./dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "start:dev": "node dist/index.js",
    "publish:release": "pnpm build && pnpm publish --access public",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "nskr",
    "cli",
    "shadcn",
    "components",
    "registry",
    "red",
    "nskred"
  ],
  "author": "Narra Suryakoushik Reddy <red@nskr.me>",
  "license": "MIT",
  "homepage": "https://nskr.me",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nskrdev/nskr.git",
    "directory": "packages/nskr"
  },
  "funding": "https://github.com/sponsors/nskred",
  "bugs": {
    "url": "https://github.com/nskrdev/nskr/issues"
  },
  "devDependencies": {
    "tsup": "^8.5.0"
  },
  "packageManager": "pnpm@10.10.0"
}